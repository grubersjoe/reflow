{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug",
      "args": [
        "--dry-run",
        "--replace-decorators",
        "src/plugin/__tests__/__fixtures__/types/generic/input.js"
      ],
      "cwd": "${workspaceRoot}",
      "env": {
        "NODE_ENV": "development"
      },
      "outFiles": ["${workspaceFolder}/build/**/*.js"],
      "program": "${workspaceRoot}/src/cli/index.ts",
      "sourceMaps": true,
      "stopOnEntry": false
    }
  ]
}
