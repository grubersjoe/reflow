{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "CLI",
      "args": [
        "--dry-run",
        "--replace-decorators",
        "src/plugin/__tests__/__fixtures__/refactoring/replace-decorators/input.js"
      ],
      "cwd": "${workspaceRoot}",
      "env": {
        "NODE_ENV": "development"
      },
      "outFiles": [
        "${workspaceFolder}/build/**/*.js",
      ],
      "program": "${workspaceRoot}/src/cli/index.ts",
      "sourceMaps": true,
      "stopOnEntry": false
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Babel Plugin",
      "args": [
        "src/plugin/__tests__/__fixtures__/react/imports/node/simple/input.js"
      ],
      "cwd": "${workspaceRoot}",
      "env": {
        "NODE_ENV": "development"
      },
      "outFiles": [
        "${workspaceFolder}/build/**/*.js",
      ],
      "program": "${workspaceRoot}/src/index.ts",
      "sourceMaps": true,
      "stopOnEntry": false
    }
  ]
}
