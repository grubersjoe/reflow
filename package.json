{
  "name": "babel-plugin-overflow",
  "version": "0.0.1",
  "description": "Babel Plugin to migrate Flow typed JavaScript to TypeScript",
  "author": "Jonathan Gruber <jogu@spreadshirt.net>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/babel-core": "^6.25.5",
    "@types/babel-traverse": "^6.25.5",
    "@types/node": "^11",
    "@typescript-eslint/eslint-plugin": "^1.1.1",
    "@typescript-eslint/parser": "^1.1.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.12.1",
    "eslint-plugin-node": "^8.0.1",
    "prettier": "^1.16.4",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "@babel/preset-flow": "^7.0.0",
    "@babel/types": "^7.3.0",
    "@types/babel-types": "^7.0.4",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "glob": "^7.1.3"
  },
  "scripts": {
    "build": "yarn run lint && yarn run prettier && tsc && webpack --mode=production --progress",
    "build:dev": "webpack --progress",
    "clean": "rm -rv build/*",
    "format": "prettier --write src/**/*",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "typecheck": "tsc",
    "typecheck:watch": "tsc -w",
    "watch": "webpack -w"
  }
}
