{
  "name": "@grubersjoe/babel-plugin-transform-flow-to-typescript",
  "version": "0.0.1",
  "description": "Babel Plugin to migrate Flow typed JavaScript to TypeScript",
  "author": "Jonathan Gruber <jogu@spreadshirt.net>",
  "license": "MIT",
  "private": true,
  "bin": {
    "overflow": "./build/overflow.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/babel__core": "^7.1.1",
    "@types/jest": "^24.0.9",
    "@types/node": "^11.13.4",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-loader": "^8.0.5",
    "eslint": "^5.16.0",
    "eslint-plugin-node": "^8.0.1",
    "flow-bin": "^0.96.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.3",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-syntax-decorators": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7",
    "@babel/plugin-syntax-flow": "^7",
    "@babel/plugin-syntax-jsx": "^7",
    "@babel/types": "^7",
    "@types/diff": "^4.0.1",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.123",
    "@types/prettier": "^1.16.1",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "diff": "^4.0.1",
    "glob": "^7.1.3",
    "lodash": "^4.17.11",
    "prettier": "^1.16.4"
  },
  "scripts": {
    "build": "webpack --mode=production --progress",
    "clean": "rm -rv build/*",
    "dev": "webpack -w",
    "format": "prettier --write 'src/**/*'",
    "lint": "eslint --max-warnings=0 'src/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage && xdg-open src/plugin/__tests__/__coverage__/lcov-report/index.html",
    "typecheck": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint"
    }
  }
}
