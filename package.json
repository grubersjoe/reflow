{
  "name": "@grubersjoe/babel-plugin-transform-flow-to-typescript",
  "version": "0.0.1",
  "description": "Babel Plugin to migrate Flow typed JavaScript to TypeScript",
  "author": "Jonathan Gruber <jogu@spreadshirt.net>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/babel__core": "^7.1.0",
    "@types/node": "^11.10.5",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-tester": "^6.0.0",
    "eslint": "^5.12.1",
    "eslint-plugin-node": "^8.0.1",
    "flow-bin": "^0.94.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-syntax-decorators": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7",
    "@babel/plugin-syntax-flow": "^7",
    "@babel/plugin-syntax-jsx": "^7",
    "@babel/types": "^7",
    "@types/diff": "^4.0.1",
    "@types/glob": "^7.1.1",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "diff": "^4.0.1",
    "glob": "^7.1.3"
  },
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --progress",
    "clean": "rm -rv build/*",
    "format": "prettier --write 'src/**/*'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc",
    "typecheck:watch": "tsc -w",
    "watch": "webpack -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint",
      "pre-push": "yarn run typecheck && yarn run lint"
    }
  }
}
